{
  "name": "react-redux-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for Redux/ReactJS project with hot code reloading and devTools enabled",
  "scripts": {
    "start": "node server.js",
    "lint": "eslint src",
    "test": "mocha --compilers js:babel-core/register --require ./test/setup.js 'test/**/*.@(js|jsx)'",
    "test:watch": "npm run test -- --watch --watch-extensions jsx"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gaearon/react-redux-boilerplate.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack"
  ],
  "author": "Patrick McGill (http://github.com/pqmcgill)",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.0.20",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.0.1",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "chai": "^3.5.0",
    "chai-immutable": "^1.6.0",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.6.2",
    "jsdom": "^9.4.1",
    "mocha": "^2.5.3",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^3.0.1",
    "redux-devtools-chart-monitor": "^1.4.1",
    "redux-devtools-diff-monitor": "^5.0.5",
    "redux-devtools-dispatch": "^2.2.0",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-inspector": "^0.6.1",
    "redux-devtools-log-monitor": "^1.0.2",
    "redux-slider-monitor": "^1.0.6",
    "style-loader": "^0.12.3",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1"
  },
  "dependencies": {
    "classnames": "^2.1.2",
    "immutable": "^3.8.1",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-redux": "^4.1.0",
    "redux": "^3.1.1"
  }
}
